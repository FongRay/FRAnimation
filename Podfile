workspace 'FRAnimation'
platform :ios, '8.0'
target 'FRAnimation' do
	pod 'Masonry', :git => 'https://github.com/SnapKit/Masonry.git', :branch => 'master'
    pod 'AFNetworking', :git => 'https://github.com/AFNetworking/AFNetworking.git'
    pod 'lottie-ios', :git => 'https://github.com/airbnb/lottie-ios.git', :branch => 'objectiveC'
    pod 'SocketRocket', :git => 'https://github.com/facebook/SocketRocket.git'
    project './FRAnimation/FRAnimation.xcodeproj'
end

